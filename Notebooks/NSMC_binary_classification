port csv\n","\n","time_step = 40\n","mecab = Mecab()\n","\n","\n","def tokenize(sentence):\n","    '''\n","      Mecab 을 이용해 각 리뷰를 POS tagging 한다.\n","    '''\n","    sw_pos_tags = ['JKS', 'JKC', 'JKG', 'JKO', 'JKB', 'JKV', 'JKQ', 'JX', 'JC',\n","               'EP', 'EF', 'EC', 'ETN', 'ETM',\n","               'SS', 'SF', 'SE', 'SP', 'SO', 'SW', 'SH', 'SL', 'SN',\n","               'IC'] # 모델 학습시 사용하지 않을 품사\n","    row_tags = ['VV', 'VA', 'VX', 'VCP', 'VCN'] # 어근(root of word) 품사\n","    sw = ['.', ',', '..', '...', '!', '?']\n","    tokenised = []\n","    pairs = mecab.pos(sentence)\n","    for morph, pos in pairs:\n","        if morph in sw: continue\n","        if pos not in sw_pos_tags:\n","            if pos in row_tags:\n","                morph += '다'\n","            tokenised.append(morph)\n","    return tokenised\n","\n","\n","def predict(sentence, model):\n","    tokens = []\n","    tokens.append(tokenize(sentence))\n","    tokens = tokenizer.texts_to_sequences(tokens)\n","    x_test = pad_sequences(tokens, maxlen=time_step)\n","    predict_result = model.predict(x_test)\n","    if predict_result[0] >= 0.5:\n","        return 1\n","    else:\n","        return 0\n","\n","\n","with open('/content/COSE461-Team-14/NSMC/ko_data.csv', 'r', encoding='CP949') as input_file:\n","    reader = list(csv.reader(input_file))\n","\n","f_o = open('result.csv', 'w')\n","writer = csv.writer(f_o)\n","writer.writerow(['Id', 'Predicted'])    \n","writer.writerow([0, 0])     # 첫 두 행 삽입\n","writer.writerow([1, 1])\n","\n","predictions = []\n","for id, review in notebook.tqdm(reader[3:]):   # 헤더 행 및 처음 2행 제외하고 시작\n","    res = predict(review, model)\n","    predictions.append([id, res])\n","\n","for id, res in notebook.tqdm(predictions):\n","    writer.writerow([id, res])\n","    \n","f_o.close()"],"execution_count":null,"outputs":[{"output_type":"display_data","data":{"application/vnd.jupyter.widget-view+json":{"model_id":"644ce5be1a624d70b2e031517e516537","version_minor":0,"v