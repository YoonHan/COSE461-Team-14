'EP', 'EF', 'EC', 'ETN', 'ETM',\n","               'SS', 'SF', 'SE', 'SP', 'SO', 'SW', 'SH', 'SL', 'SN',\n","               'IC'] # 모델 학습시 사용하지 않을 품사\n","    row_tags = ['VV', 'VA', 'VX', 'VCP', 'VCN'] # 어근(root of word) 품사\n","    sw = ['.', ',', '..', '...', '!', '?']\n","    tokenised = []\n","    pairs = mecab.pos(sentence)\n","    for morph, pos in pairs:\n","        if morph in sw: continue\n","        if pos not in sw_pos_tags:\n","            if pos in row_tags:\n","                morph += '다'\n","            tokenised.append(morph)\n","    \n","    return tokenised\n","        \n","\n","# train, test dataset 에 'tokens' column을 만들고 거기에 토큰화된 리뷰 데이터를 넣는다.\n","tokenised_review = []\n","for review in notebook.tqdm(train_df['document']):\n","    tokenised_review.append(tokenize(review))\n","train_df['tokens'] = tokenised_review\n","\n","tokenised_review = []\n","for review in notebook.tqdm(test_df['document']):\n","    tokenised_review.append(tokenize(review))\n","test_df['tokens'] = tokenised_review\n","\n","# 토큰화가 잘 이루어졌는지 확인\n","display(train_df.head(10))\n","display(test_df.head(10))"],"execution_count":null,"outputs":[{"output_type":"display_data","data":{"application/vnd.jupyter.widget-view+json":{"model_id":"cc86bfcd291a4ff79562c0b08e0bb680","version_minor":0,"version_major":2},"text/plain":["HBox(children=(FloatProgress(value=0.0, max=149995.0), HTML(value='')))"]},"metadata":{"tags":[]}},{"output_type":"stream","text":["\n"],"name":"stdout"},{"output_type":"display_data","data":{"application/vnd.jupyter.widget-view+json":{"model_id":"f27f26e574e9434fa69cae6a9b8a2f68","version_minor":0,"version_major":2},"text/plain":["HBox(children=(FloatProgress(value=0.0, max=49997.0), HTML(value='')))"]},"metadata":{"tags":[]}},{"output_type":"stream","text":["\n"],"name":"stdout"},{"output_type":"display_data","data":{"text/html":["<div>\n","<style scoped>\n","    .dataframe tbody tr th:only-of-type {\n","        vertical-align: middle;\n","    }\n","\n","    .dataf